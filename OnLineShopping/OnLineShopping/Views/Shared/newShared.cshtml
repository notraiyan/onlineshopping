<!DOCTYPE html>
<html>
<head>
    <title>Welcome to online shop</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no">
    @Styles.Render("~/assets/css/bootstrap.min.css")
    @Styles.Render("~/font-awesome/css/font-awesome.min.css")
    @Styles.Render("~/AdminLTE/bower_components/Ionicons/css/ionicons.min.css")
    @Styles.Render("~/AdminLTE/bower_components/jvectormap/jquery-jvectormap.css")
    @Styles.Render("~/AdminLTE/dist/css/AdminLTE.min.css")
    @Styles.Render("~/AdminLTE/dist/css/skins/_all-skins.min.css")
    @Styles.Render("~/AdminLTE/bower_components/datatables.net-bs/css/datatables.bootstrap.min.css")
    @Styles.Render("~/AdminLTE/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css")
    @Styles.Render("~/assets/css/paper-bootstrap-wizard.css")
    @Styles.Render("~/assets/css/demo.css")
    @Styles.Render("~/Custom/style.css")


    @RenderSection("Styles", false)
    @RenderSection("Scripts", false)
    @Scripts.Render("~/assets/js/jquery-2.2.4.min.js")
    @Scripts.Render("~/assets/js/bootstrap.min.js")
    @Scripts.Render("~/assets/js/jquery.validate.min.js")

    @Scripts.Render("~/AdminLTE/bower_components/fastclick/lib/fastclick.js")
    @Scripts.Render("~/AdminLTE/dist/js/adminlte.min.js")
    @Scripts.Render("~/AdminLTE/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js")
    @Scripts.Render("~/AdminLTE/bower_components/jquery-slimscroll/jquery.slimscroll.min.js")
    @Scripts.Render("~/AdminLTE/dist/js/demo.js")
    @Scripts.Render("~/AdminLTE/bower_components/datatables.net/js/jquery.dataTables.min.js")
    @Scripts.Render("~/AdminLTE/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js")
    @Scripts.Render("~/AdminLTE/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js")
</head>
<body class="hold-transition skin-blue sidebar-collapse sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                
                
        </header>

        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->
        


        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                    <small></small>
                </h1>
                
            </section>


            <section class="content">

                @RenderBody()
            </section>

        </div>


        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright &copy; 2014-2016 <a href="http://data-edgeo.com">data edge</a>.</strong> All rights
            reserved.
        </footer>

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane" id="control-sidebar-home-tab">
                    <h3 class="control-sidebar-heading">Recent Activity</h3>
                    <ul class="control-sidebar-menu"></ul>
                    <!-- /.control-sidebar-menu -->

                    <h3 class="control-sidebar-heading">Tasks Progress</h3>
                    <ul class="control-sidebar-menu">


                        <li>
                            <a href="javascript:void(0)">
                                <h4 class="control-sidebar-subheading">
                                    Email Notification
                                    <span class="label label-danger pull-right">00%</span>
                                </h4>

                                <div class="progress progress-xxs">
                                    <div class="progress-bar progress-bar-danger" style="width: 00%"></div>
                                </div>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:void(0)">
                                <h4 class="control-sidebar-subheading">
                                    Testing
                                    <span class="label label-danger pull-right">5%</span>
                                </h4>

                                <div class="progress progress-xxs">
                                    <div class="progress-bar progress-bar-primary" style="width: 5%"></div>
                                </div>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="tab-pane" id="control-sidebar-stats-tab">Stats Tab Content</div>
                <div class="tab-pane" id="control-sidebar-settings-tab">
                </div>
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div>
    
</body>
</html>
